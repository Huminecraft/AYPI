package com.aypi.utils.xml.balises;

import java.util.jar.Attributes;

import org.bukkit.entity.Player;

import com.aypi.utils.xml.MCBalise;

public class PlayerJoinBalise extends MCBalise {

	private final static String name = "on-player-join";
	
	public PlayerJoinBalise() {
		super(name);
		
	}

	@Override
	public void setUpAttributes(Attributes attributes) {
		
	}

	@Override
	public void execute(Player player) {
		for (MCBalise mcb : getChildrens()) {
			mcb.execute(player);
		}
	}

	
	
}
