package com.aypi.utils.xml;

import org.bukkit.entity.Player;
import org.xml.sax.Attributes;

import com.aypi.Aypi;

public abstract class MCBalise {
	
	private String name;
	private String containt;
	
	public MCBalise(String name) {
		this.name = name;
		Aypi.getXMLFileManager().addMCBalise(this);
	}
	
	public abstract void load(Attributes attributes);
	
	public abstract void execute(Player player);
	
	public String getName() {
		return name;
	}
	
	public void setContaint(String value) {		
		this.containt = value;
	}
	
	public String getContaint() {
		return this.containt;
	}
	
}
